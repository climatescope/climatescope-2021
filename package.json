    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-rev": "^8.1.1",
    "gulp-rev-rewrite": "^1.1.1",
    "gulp-sass": "^4.0.1",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.2",
    "jeet": "^7.2.0",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.9.0",
    "require-globify": "^1.4.1",
    "run-sequence": "^2.2.1",
    "surge": "^0.20.1",
